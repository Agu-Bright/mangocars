    "@capacitor/ios": "^5.1.0",
    "@capacitor/keyboard": "^5.0.5",
    "@capacitor/splash-screen": "^5.0.5",
    "@capacitor/status-bar": "^5.0.5",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@heroicons/react": "^2.0.18",
